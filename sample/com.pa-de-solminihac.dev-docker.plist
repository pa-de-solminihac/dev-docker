<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>EnvironmentVariables</key>
        <dict>
            <key>PATH</key>
            <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/opt/X11/bin:~/bin:/usr/local/sbin</string>
        </dict>
        <key>Label</key>
        <string>com.pa-de-solminihac.dev-docker</string>
        <key>ProgramArguments</key>
        <array>
            <string>/path/to/dev-docker/start.sh</string>
            <string>--no-bg</string>
        </array>
        <key>RunAtLoad</key>
        <true/>
    </dict>
</plist>
