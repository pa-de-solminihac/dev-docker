# commands based on config values
DOCKER_ENV_VARS_CMD_OPTS="env --shell bash $DEVDOCKER_VM"
DOCKER_ENV_VARS="$("$DOCKERMACHINE" $DOCKER_ENV_VARS_CMD_OPTS)"
DOCKERMACHINEIP="$("$DOCKERMACHINE" ip $DEVDOCKER_VM)"
# override SSH_CMD so it connects to VM instead
SSH_CMD="ssh -i $SSH_PRIVKEY -q -p 8022 -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -o ControlMaster=yes devdocker@$DOCKERMACHINEIP"
SSH_PORT_FW_CMD="$SSH_CMD \
    -L 0.0.0.0:80:$DOCKERMACHINEIP:80 \
    -L 0.0.0.0:443:$DOCKERMACHINEIP:443 \
    -L 0.0.0.0:3306:$DOCKERMACHINEIP:3306 \
    -L 0.0.0.0:9200:$DOCKERMACHINEIP:9200 \
    -L 0.0.0.0:9300:$DOCKERMACHINEIP:9300 \
    -L 127.0.0.2:80:$DOCKERMACHINEIP:6081 \
    -L 0.0.0.0:6081:$DOCKERMACHINEIP:6081 \
    -L 0.0.0.0:6082:$DOCKERMACHINEIP:6082 \
    -L 127.0.0.2:6081:$DOCKERMACHINEIP:6081 \
    -L 0.0.0.0:5601:$DOCKERMACHINEIP:5601 \
    -N "
